{
  "license": "MIT",
  "author": {
    "name": "Christian Konnaris"
  },
  "name": "console-log-remover",
  "displayName": "Console.log Remover",
  "description": "Remove and toggle all console logs in the current file",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "console-log-remover.remove",
        "title": "Remove console logs"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "1.107.0",
    "@vscode/test-cli": "0.0.12",
    "@vscode/test-electron": "2.5.2",
    "eslint": "9.39.1",
    "prettier": "3.7.4",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.1"
  }
}
